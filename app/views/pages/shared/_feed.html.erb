<ul class="mf-posts grid p-6">
     <% @posts.each do |post| %>
          <div class="post-action-items border border-2 rounded pt-2 text-center grid justify-center">
               <li><%= render partial: "posts/shared/meme_post", locals: { post: post } %></li>
               <li class="self-center text-sm"><%= render partial: "posts/shared/edit_or_delete", locals: { post: post } %></li>
          </div>
     <% end %>
</ul>